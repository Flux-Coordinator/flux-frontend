{
	"name": "flux-frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"concurrently": "^3.5.1",
		"cross-env": "^5.1.4",
		"material-ui": "^1.0.0-beta.37",
		"material-ui-icons": "^1.0.0-beta.36",
		"react": "16.0.0",
		"react-dom": "16.0.0",
		"react-scripts": "1.1.1",
		"typeface-roboto": "^0.0.54"
	},
	"scripts": {
		"start-all":
			"cross-env NODE_PATH=src concurrently \"react-scripts start\" \"start-storybook -p 9001 -c .storybook\"",
		"start": "cross-env NODE_PATH=src react-scripts start",
		"build": "cross-env NODE_PATH=src react-scripts build",
		"test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
		"test-ci":
			"cross-env NODE_PATH=src react-scripts test --env=jsdom --coverage",
		"eject": "cross-env NODE_PATH=src react-scripts eject",
		"storybook": "start-storybook -p 9001 -c .storybook"
	},
	"devDependencies": {
		"@storybook/react": "3.3.15",
		"babel-core": "^6.26.0",
		"babel-eslint": "^8.2.2",
		"eslint": "4.9.0",
		"eslint-config-airbnb": "16.1.0",
		"eslint-config-prettier": "^2.9.0",
		"eslint-plugin-import": "2.7.0",
		"eslint-plugin-jsx-a11y": "6.0.2",
		"eslint-plugin-prettier": "^2.6.0",
		"eslint-plugin-react": "7.4.0",
		"flow-bin": "^0.67.1",
		"husky": "^0.15.0-rc.9",
		"install-peerdeps": "^1.6.0",
		"prettier": "1.11.1",
		"prettier-eslint": "^8.8.1",
		"pretty-quick": "^1.4.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	}
}
